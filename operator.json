{
  "name": "FAUST",
  "description": "Full annotation using shape-constrained trees, a clustering and phenotyping algorithm for flow cytometry data.",
  "tags": [
    "flow cytometry",
    "clustering",
    "dimensionality reduction"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/faust_operator"
  ],
  "container": "tercen/faust_operator:0.1.2",
  "properties": [
    {
      "kind": "DoubleProperty",
      "name": "selectionQuantile",
      "defaultValue": 0.5,
      "description": "Selection quantile. See FAUST documentation."
    },
    {
      "kind": "DoubleProperty",
      "name": "depthScoreThreshold",
      "defaultValue": 0.01,
      "description": "Depth score threshold. See FAUST documentation."
    },
    {
      "kind": "DoubleProperty",
      "name": "seed",
      "defaultValue": 123,
      "description": "Random seed."
    }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Variables",
            "type": "",
            "description": "Represents the variables (e.g. channels, markers)",
            "ontologyMapping": "variable",
            "crosstabMapping": "row",
            "cardinality": "1..n",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "Clusters",
            "type": "",
            "description": "Represents the clusters (e.g. cells)",
            "ontologyMapping": "observation",
            "crosstabMapping": "column",
            "cardinality": "1..n",
            "factors": []
          }
        ],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Measurement",
                "type": "numeric",
                "description": "Value of measurement signal of the channel/marker",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              }
            ]
          }
        ]
      }
    ],
    "outputSpecsV2": [
      {
        "kind": "OperatorJoinSpec",
        "description": "FAUST clustering with UMAP coordinates and phenotype gates",
        "joinOperators": [
          {
            "kind": "JoinOperator",
            "joinType": "left",
            "leftPair": {
              "kind": "ColumnPair",
              "lColumns": [
                "Clusters"
              ],
              "rColumns": [
                "Clusters"
              ]
            },
            "rightRelation": {
              "kind": "TableRelation",
              "description": "Output includes umap coordinates, label, and dynamic gate columns per marker",
              "attributes": [
                {
                  "kind": "Attribute",
                  "name": "umap_1",
                  "type": "double"
                },
                {
                  "kind": "Attribute",
                  "name": "umap_2",
                  "type": "double"
                },
                {
                  "kind": "Attribute",
                  "name": "label",
                  "type": "string"
                }
              ]
            }
          }
        ]
      }
    ]
  }
}